service: authentication-service

runtime: java
env: flex

runtime_config:
  jdk: openjdk8

handlers:
- url: /.*
  script: this field is required, but ignored

manual_scaling:
  instances: 1

health_check:
  enable_health_check: False

resources:
  cpu: 1
  memory_gb: 2.3
  disk_size_gb: 10

env_variables:
  DB_HOST:
  DB_DATABASE:
  DB_USERNAME:
  DB_PASSWORD:
  JWT_SECRET:
  JWT_EXPIRESEC: 86400